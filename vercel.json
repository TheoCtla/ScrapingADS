{
    "version": 2,
    "builds": [],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "https://TON-BACKEND-HÉBERGÉ.com/api/$1"
      }
    ],
    "routes": [
      {
        "handle": "filesystem"
      },
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "outputDirectory": "dist",
    "rootDirectory": "frontend"
  }


